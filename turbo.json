{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["lint", "build"],
      "outputs": []
    },
    "docs": {
      "dependsOn": ["build"],
      "outputs": ["docs/**"]
    },
	"lint": {}
  }
}
